{
  "name": "mnee-marketplace",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --filter=./apps/*",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "check-types": "turbo run check-types",
    "deploy": "turbo run deploy --filter=./apps/*",
    "preview": "turbo run preview --filter=./apps/*",
    "destroy": "turbo run destroy",
    "prepare": "lefthook install"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@cloudflare/workers-types": "^4.20251223.0",
    "lefthook": "^2.0.12",
    "turbo": "^2.7.1",
    "typescript": "5.9.3",
    "ultracite": "6.5.0"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.3.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@langchain/anthropic": "^1.3.3",
    "@langchain/core": "^1.1.8",
    "@langchain/openai": "^1.2.0",
    "alchemy": "^0.82.1",
    "viem": "^2.43.3",
    "zod": "^4.2.1"
  }
}
